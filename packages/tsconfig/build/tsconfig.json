{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "ES2022",
    "strict": true,
    "allowJs": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true
  }
}
